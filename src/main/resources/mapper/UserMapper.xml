<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.mvtech.mapper.UserMapper">
	<select id="findAll" resultType="user">
		select * from user
	</select>
	
	<insert id="addUser" parameterType="Map">
		insert into user(id,name,age,sex,address)
		values(#{id},#{name},#{age},#{sex},#{address})
	</insert>
	
	<insert id="addUserTest">
	insert into test1(a,b) values(#{i},#{j});
	</insert>
	
	<select id="findUser"  parameterType="Map" resultType = "Map">
		select * from flowActionTrace
	</select>
</mapper>